datasource db {
  provider = "sqlite"
  url      = env("DATABASE_URL")
}

generator client {
  provider = "prisma-client-js"
}

model User {
  id    Int    @id @default(autoincrement())
  password String
  name  String
  email String @unique
}

model Admin {
  id    Int    @id @default(autoincrement())
  password String
  name  String
  email String @unique
}